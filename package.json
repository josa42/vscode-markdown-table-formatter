{
    "name": "markdown-table-formatter",
    "displayName": "Markdown Table Formatter",
    "description": "",
    "version": "0.2.3",
    "publisher": "josa",
    "license": "MIT",
    "homepage": "https://github.com/josa42/vscode-markdown-table-formatter",
    "repository": {
        "type": "git",
        "url": "https://github.com/josa42/vscode-markdown-table-formatter.git"
    },
    "bugs": {
        "url": "https://github.com/josa42/vscode-markdown-table-formatter/issues"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other",
        "Formatters"
    ],
    "icon": "docs/assets/icon-128x128.png",
    "galleryBanner": {
        "color": "#FFFFFF",
        "theme": "light"
    },
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Markdown Table Formatter",
            "properties": {
                "markdownTableFormatter.enable": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable/disable Markdown Table Formatter."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.6.9",
        "glob": "^7.1.4",
        "mocha": "^6.2.0",
        "typescript": "^3.5.3",
        "vscode": "^1.1.0",
        "vscode-test": "^1.1.0"
    },
    "dependencies": {
        "escape-string-regexp": "^2.0.0"
    }
}
